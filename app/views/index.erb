<div class="container">
  <h1>User Skills and Levels</h1>
  <table>
    <tr>
      <th>Name</th>
      <th>Skill</th>
      <th>Experience</th>
      <th>Formal?</th>
    </tr>
  <% @users.each do |user| %>
    <% user.proficiencies.each do |prof| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= prof.skill.name %></td>
        <td><%= prof.experience %></td>
        <td><%= prof.formal_education %></td>
      </tr>
    <% end %>
    
  <% end %>
<table>
  <%# TODO: display the skills and proficiency levels for each user ... %>
</div>
